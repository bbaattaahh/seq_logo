package org;

public class TheTypeOfTheSections {
	
	public int one_or_two_end_pont_in_the_other_section (Integer a, Integer b,Integer c,Integer d){

		Integer counter=0;
		
		if (a>c && a<d) counter++; 
		if (b>c && b<d) counter++; 
		
		if (a==c) counter++;
		if (a==d) counter++;
		if (b==c) counter++;
		if (b==d) counter++;
		
		if (a==b) counter--;
		if (c==d) counter--;
		
		if (a==b && b==c && c==d) counter--;
	    return counter;
	}

}
