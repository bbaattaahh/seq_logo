package org.mbk;

public class Run {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Proba elso    = new Proba();
		Proba masodik = new Proba();
		String szoveg = "hello";
		
		elso.a = 20;
		elso.inc();
		elso.inc();
		elso.showCounter();
		masodik.showCounter();
		if(szoveg.equals("hello")){
			System.out.println("Equals");
		}
		else{
			System.out.println("Not equal");
		}
		System.out.println(szoveg);
	}

}
